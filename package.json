{
	"name": "torx",
	"displayName": "Torx",
	"description": "Torx syntax and snippets.",
	"version": "0.2.1",
	"publisher": "slulego",
	"engines": {
		"vscode": "0.10.x"
	},
	"categories": [
		"Programming Languages",
		"Formatters",
		"Snippets"
	],
	"activationEvents": [
		"onLanguage:torx"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/slulego/vscode-torx.git"
	},
	"icon": "images/icon.png",
	"galleryBanner": {
		"color": "#111111",
		"theme": "dark"
	},
	"main": "./index.js",
	"scripts": {
		"update-grammar": "node ../../build/npm/update-grammar.js"
	},
	"contributes": {
		"commands": [
			{
				"command": "extension.torx",
				"title": "Format Torx Files"
			}
		],
		"languages": [
			{
				"id": "torx",
				"extensions": [
					".torx"
				],
				"aliases": [
					"Torx",
					"torx"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "torx",
				"scopeName": "text.html.torx",
				"path": "./syntaxes/torx.tmLanguage.json",
				"embeddedLanguages": {
					"text.html.basic": "html"
				}
			}
		],
		"snippets": [
			{
				"language": "torx",
				"path": "./snippets/torx.snippets.json"
			}
		]
	}
}