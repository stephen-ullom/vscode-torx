{
   "name": "torx",
   "displayName": "Torx",
   "description": "Torx syntax and snippets.",
   "version": "1.0.2",
   "publisher": "slulego",
   "engines": {
      "vscode": "^1.14.0"
   },
   "categories": [
      "Programming Languages",
      "Formatters",
      "Snippets"
   ],
   "repository": {
      "type": "git",
      "url": "https://github.com/torxjs/vscode-torx.git"
   },
   "icon": "images/icon.png",
   "galleryBanner": {
      "color": "#111111",
      "theme": "dark"
   },
   "main": "dist/extension.js",
   "contributes": {
      "languages": [
         {
            "id": "torx",
            "extensions": [
               ".torx"
            ],
            "aliases": [
               "Torx",
               "torx"
            ],
            "configuration": "./language-configuration.json"
         }
      ],
      "grammars": [
         {
            "language": "torx",
            "scopeName": "text.torx",
            "path": "./syntaxes/torx.tmLanguage.json"
         }
      ],
      "snippets": [
         {
            "language": "torx",
            "path": "./snippets/torx.snippets.json"
         }
      ],
      "activationEvents": [
         "onLanguage:torx"
      ]
   },
   "devDependencies": {
      "@types/node": "^16.7.10",
      "@types/vscode": "^1.14.0"
   }
}
